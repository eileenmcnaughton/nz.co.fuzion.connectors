<?xml version="1.0"?>
<extension key="nz.co.fuzion.connectors" type="module">
  <file>connectors</file>
  <name>External connectors support</name>
  <description>This extension manages information about external connector credentials</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Eileen</author>
    <email>eileen@fuzion.co.nz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-03-30</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.78</ver>
  </compatibility>
  <comments>This is a supporting extension for managing information for connecting to external services.</comments>
  <civix>
    <namespace>CRM/Connectors</namespace>
    <format>24.09.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Connectors\AutomaticUpgrader</upgrader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
